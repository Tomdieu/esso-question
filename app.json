{
  "expo": {
    "name": "esso-question",
    "slug": "esso-question",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "install": {
      "exclude": ["expo-updates", "expo-splash-screen","react-native-reanimated"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash-icon.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",{
          "fonts":["./assets/fonts/Inter-Medium.ttf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
