{
  "expo": {
    "name": "esso",
    "slug": "esso",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon.jpg",
    "scheme": "esso",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.esso.idp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.esso.idp"
    },
    "install": {
      "exclude": [
        "expo-updates",
        "expo-splash-screen",
        "react-native-reanimated"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash-icon.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter-Medium.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
